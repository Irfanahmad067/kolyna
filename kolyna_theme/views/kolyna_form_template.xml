                                                        <option t-att-value="offer[0]" selected="true"><t t-esc="offer[1]"/></option>
                                                    </t>
                                                    <t t-else="">
                                                        <option t-att-value="offer[0]"><t t-esc="offer[1]"/></option>
                                                    </t>
                                                </t>
                                            </select>
