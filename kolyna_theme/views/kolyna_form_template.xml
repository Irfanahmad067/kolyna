                                        <label class="ration_label">
                                            Ration Quotidienne (GR)
                                        </label>
                                        <div class="ration_value">
                                            <input type='text' value="50" name="daily_ration_g" style="display: none;"/>
                                            <label class="value_label ration_value">0</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="opt_footer">
                                    <button current-id="1" class="btn btn-next" type="button">Next Page >>
                                    </button>
