                                                <label class="control-text text-muted radio-inline">
                                                    <t t-if="desired_ration_default == ration[0]">
                                                        <input type="radio" t-attf-name="ration_type" t-att-value="ration[0]" checked="checked"/>
                                                    </t>
                                                    <t t-else="">
                                                        <input type="radio" t-attf-name="ration_type" t-att-value="ration[0]"/>
                                                    </t>
                                                    <t t-esc="ration[1]"/>
                                                </label>
